<% content_for :page_title do %>
  <%= link_to Spree.t(:legal_pages), admin_legal_pages_url %> /
  <%= @legal_page.title %>
<% end %>

<%= render partial: 'spree/admin/shared/error_messages', locals: { target: @legal_page } %>

<%= form_for [:admin, @legal_page] do |f| %>
  <fieldset>
    <%= render partial: 'form', locals: { f: f } %>
    <%= render partial: 'spree/admin/shared/edit_resource_links' %>
  </fieldset>
<% end %>
