<% content_for :page_title do %>
  <%= link_to Spree.t(:sponsorships), admin_sponsorships_url %> /
  <%= @sponsorship.name %>
<% end %>

<%= render partial: 'spree/admin/shared/error_messages', locals: { target: @sponsorship } %>

<%= form_for [:admin, @sponsorship] do |f| %>
  <fieldset>
    <%= render partial: 'form', locals: { f: f } %>
    <%= render partial: 'spree/admin/shared/edit_resource_links' %>
  </fieldset>
<% end %>
