<% content_for :page_title do %>
  <%= link_to Spree.t(:questions_categories), admin_questions_categories_url %> /
  <%= @questions_category.name %>
<% end %>

<%= render partial: 'spree/admin/shared/error_messages', locals: { target: @questions_category } %>

<%= form_for [:admin, @questions_category] do |f| %>
  <fieldset>
    <%= render partial: 'form', locals: { f: f } %>
    <%= render partial: 'spree/admin/shared/edit_resource_links' %>
  </fieldset>
<% end %>
