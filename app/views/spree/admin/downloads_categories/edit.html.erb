<% content_for :page_title do %>
  <%= link_to Spree.t(:downloads_categories), admin_downloads_categories_url %> /
  <%= @downloads_category.name %>
<% end %>

<%= render partial: 'spree/admin/shared/error_messages', locals: { target: @downloads_category } %>

<%= form_for [:admin, @downloads_category] do |f| %>
  <fieldset>
    <%= render partial: 'form', locals: { f: f } %>
    <%= render partial: 'spree/admin/shared/edit_resource_links' %>
  </fieldset>
<% end %>
