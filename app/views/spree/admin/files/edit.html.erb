<% content_for :page_title do %>
  <%= link_to Spree.t(:files), admin_files_url %> /
  <%= @file.name %>
<% end %>

<%= render partial: 'spree/admin/shared/error_messages', locals: { target: @file } %>

<%= form_for [:admin, @file] do |f| %>
  <fieldset>
    <%= render partial: 'form', locals: { f: f } %>
    <%= render partial: 'spree/admin/shared/edit_resource_links' %>
  </fieldset>
<% end %>
