<% content_for :page_title do %>
  <%= link_to Spree.t(:property_value_icons), admin_property_value_icons_url %> /
  <%= @property_value_icon.property.try(:name) %>: <%= @property_value_icon.value %>
<% end %>

<%= render partial: 'spree/admin/shared/error_messages', locals: { target: @property_value_icon } %>

<%= form_for [:admin, @property_value_icon] do |f| %>
  <fieldset>
    <%= render partial: 'form', locals: { f: f } %>
    <%= render partial: 'spree/admin/shared/edit_resource_links' %>
  </fieldset>
<% end %>
