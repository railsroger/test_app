<% content_for :page_title do %>
  <%= link_to Spree.t(:news), admin_news_index_url %> /
  <%= @news.name %>
<% end %>

<%= render partial: 'spree/admin/shared/error_messages', locals: { target: @news } %>

<%= form_for [:admin, @news] do |f| %>
  <fieldset>
    <%= render partial: 'form', locals: { f: f } %>
    <%= render partial: 'spree/admin/shared/edit_resource_links' %>
  </fieldset>
<% end %>
