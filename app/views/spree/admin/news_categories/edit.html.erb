<% content_for :page_title do %>
  <%= link_to Spree.t(:news_categories), admin_news_categories_url %> /
  <%= @news_category.name %>
<% end %>

<%= render partial: 'spree/admin/shared/error_messages', locals: { target: @news_category } %>

<%= form_for [:admin, @news_category] do |f| %>
  <fieldset>
    <%= render partial: 'form', locals: { f: f } %>
    <%= render partial: 'spree/admin/shared/edit_resource_links' %>
  </fieldset>
<% end %>
